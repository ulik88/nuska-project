{
  "expo": {
    "name": "nuska",
    "slug": "nuska",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/Logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {},
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ulikk.nuska"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png"
      },
      "package": "com.ulikk.nuska"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
