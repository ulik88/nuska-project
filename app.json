{
  "expo": {
    "name": "nuska",
    "slug": "nuska",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/Logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#bd2d2d",
      "splashImage": "./assets/img/splash.png"

    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.nuska",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
